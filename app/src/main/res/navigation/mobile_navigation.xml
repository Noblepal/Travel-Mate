    <fragment
        android:id="@+id/nav_visited"
        android:name="com.trichain.omiinad.fragments.VisitedFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_visited" />

    <fragment
        android:id="@+id/nav_home"
        android:name="com.trichain.omiinad.fragments.HomeFragment"
